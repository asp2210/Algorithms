   "source": [
    "def bubblesort(a):\n",
    "     i=0\n",
    "     swap=1\n",
    "     n=len(a)\n",
    "     while(swap>0):\n",
    "         swap=0\n",
    "         for i in range(n-1):\n",
    "            if a[i]>a[i+1]:\n",
    "                a[i],a[i+1]=a[i+1],a[i]\n",
    "                swap+=1\n",
    "     return a"
   ]
  },
